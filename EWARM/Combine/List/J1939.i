      }
    }
}




















 
int8_t J1939_Initialization( uint8_t InitNAMEandAddress)
{
    
    
  
    
  
    if((J1939_Address >= 0x80) && (J1939_Address <= 247U)  && (InitNAMEandAddress == J1939_TRUE))
    {
      J1939_state = J1939_HAVE_ADDR;
    }else{      
      J1939_state = J1939_IDLE;
      J1939_Address = 254U;
    }
    J1939_Address_pref = 255U;
    
    if (InitNAMEandAddress)
    {
      J1939_Address_pref = 0x80;
      CA_Name[7] = (0x80 | (3 << 4) | 0);
      CA_Name[6] = (0 << 1);
      CA_Name[5] = 0x88;
      CA_Name[4] = ((0 << 3) | 0);
      CA_Name[3] = (0 >> 3);
      CA_Name[2] = (((0 & 0x07) << 5) | (193399 >> 16));
      CA_Name[1] = ((193399 >> 8) & 0xFF);
      CA_Name[0] = (193399 & 0xFF);
    }
    
    memset(sc_addrs, 1, sizeof(sc_addrs));
    
    
    
    
    if((J1939_Address >= 0x80) && (J1939_Address <= 247U) && (InitNAMEandAddress == J1939_TRUE) ){
      
      j1939_send_addr_claimed(J1939_Address);
      return 0;
    }else{
      return j1939_claim_addr();
    }
    
    
    
}



