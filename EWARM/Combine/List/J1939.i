    }
        
     
    if((rx->DLC == 3) && (rx_data[0] == req_addr_claimed_mesg.data[0]) &&
       (rx_data[1] == req_addr_claimed_mesg.data[1]) && (rx_data[2] == req_addr_claimed_mesg.data[2])){
       
       
      if(((rx->ExtId >> 8) & 0xFFU) == J1939_Address || ((rx->ExtId >> 8) & 0xFFU) == 255U) {
        
